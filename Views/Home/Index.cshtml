

@{ ViewData["Title"] = "Home Page"; }
@model IEnumerable<FRUTERIA.Models.FRUTERIA_MAR>

    <div class="text-center">
        <h1>♣ Bienvenido a FRUTERÍA MAR ♣</h1>
    </div>


    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>


        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img style="width: auto; height:auto; position:relative; margin-left:170px;" class="d-block w-75" src="~/Imagenes/Limon.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img style="width: auto; height:auto; position:relative; margin-left:240px;" class="d-block w-60" src="~/Imagenes/cereza.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img style="width: auto; height:auto; position:relative; margin-left:170px;" class="d-block w-75" src="~/Imagenes/coco.jpg" alt="Third slide">
            </div>
            <div class="carousel-item">
                <img style="width: auto; height:auto; position:relative; margin-left:170px;" class="d-block w-75" src="~/Imagenes/mandarina.jpg" alt="Fourth slide">
            </div>
            <div class="carousel-item">
                <img style="width: auto; height:auto; position:relative; margin-left:170px;" class="d-block w-75" src="~/Imagenes/guayaba.jpg" alt="Fifth slide">
            </div>
            <div class="carousel-item">
                <img style="width: auto; height:auto; position:relative; margin-left:170px;" class="d-block w-75" src="~/Imagenes/mango.jpg" alt="Sixth slide">
            </div>

        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
